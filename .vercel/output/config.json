{
  "version": 3,
  "overrides": {
    "contact/index.html": {
      "path": "contact"
    },
    "products/index.html": {
      "path": "products"
    },
    "categories/index.html": {
      "path": "categories"
    },
    "index.html": {
      "path": ""
    }
  },
  "routes": [
    {
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "src": "/_nuxt/builds/meta/(.*)"
    },
    {
      "headers": {
        "Content-Type": "application/xslt+xml"
      },
      "src": "/__sitemap__/style.xsl"
    },
    {
      "headers": {
        "cache-control": "public, max-age=1, immutable"
      },
      "src": "/_nuxt/builds/(.*)"
    },
    {
      "status": 301,
      "headers": {
        "Location": "/produkt/$1"
      },
      "src": "/product/(.*)"
    },
    {
      "status": 301,
      "headers": {
        "Location": "/produkt-kategoriya/$1"
      },
      "src": "/product-category/(.*)"
    },
    {
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "src": "/_nuxt/(.*)"
    },
    {
      "src": "/_nuxt/builds/meta(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/_nuxt/builds(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/_nuxt(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/__nuxt_error",
      "dest": "/__nitro"
    },
    {
      "src": "/categories",
      "dest": "/__nitro"
    },
    {
      "src": "/contact",
      "dest": "/__nitro"
    },
    {
      "src": "(?<url>/checkout/.*)",
      "dest": "/__nitro"
    },
    {
      "src": "/cart",
      "dest": "/__nitro"
    },
    {
      "src": "(?<url>/my-account/.*)",
      "dest": "/__nitro"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml?url=$url"
    },
    {
      "src": "/(.*)",
      "dest": "/__nitro"
    }
  ]
}